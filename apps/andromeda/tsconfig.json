{
 "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"],
    },
    "noEmit": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
}